{
  "name": "@civi/legislation-data",
  "version": "0.1.0",
  "scripts": {
    "get-cache": "npx ts-node cache-grabber/run",
    "scrape": "npx ts-node scrapers/run",
    "gpt": "npx ts-node gpt-summaries/run",
    "wiki": "npx ts-node wiki-summaries/run",
    "typecheck": "npx tsc",
    "prettier": "prettier '**/*.{js,ts,jsx,tsx,json,css,md}'",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "lint": "eslint '**/*.ts' --ignore-pattern node_modules/",
    "spellcheck": "cspell '**/*.{js,ts,jsx,tsx,json,css,md}'",
    "quality": "npm run typecheck && npm run lint && npm run format:check && npm run spellcheck"
  },
  "main": "dist_api/index.js",
  "devDependencies": {
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "cspell": "^8.14.2",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "date-fns": "^3.3.1"
  }
}
